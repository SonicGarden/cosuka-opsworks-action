{
  "name": "cosuka-opsworks-action",
  "version": "1.2.4",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SonicGarden/cosuka-opsworks-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "aki77",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.0.0",
    "@aki77/actions-replace-comment": "^0.5.2",
    "execa": "^5.1.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/node": "^16",
    "@typescript-eslint/parser": "^5.33.1",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^8.22.0",
    "eslint-plugin-github": "^4.2.0",
    "eslint-plugin-jest": "^26.8.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.3",
    "jest-circus": "^28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "^2.3.2",
    "ts-jest": "^28.0.8",
    "typescript": "4.7.4"
  }
}
